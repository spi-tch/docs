{
  "name": "Spitch",
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "colors": {
    "primary": "#000000",
    "light": "#FFFFFF",
    "dark": "#000000"
  },
  "favicon": {
    "light": "/favicon/light.png",
    "dark": "/favicon/dark.png"
  },
  "icons": {
    "library": "fontawesome"
  },
  "navbar": {
    "links": [
      {
        "label": "Community",
        "href": "https://discord.gg/2S7zgKGhPj",
        "icon": "discord"
      },
      {
        "label": "Support",
        "href": "mailto:dev@spitch.app",
        "icon": "envelope"
      },
      {
        "label": "Blog",
        "href": "https://spitch.app/blog",
        "icon": "bookmark"
      },
      {
        "label": "Studio",
        "href": "https://spitch.studio",
        "icon": "microphone-lines"
    }
    ]
  },
  "navigation": {
    "dropdowns": [
      {
        "dropdown": "Documentation",
        "icon": "books",
        "description": "developer docs",
        "groups": [
          {
            "group": "Get Started",
            "icon": "cloud-sun",
            "pages": [
              "index",
              "start/quickstart",
              "start/installation",
              "start/livekit",
              "start/models"
            ]
          },
          {
            "group": "Features",
            "icon": "message-code",
            "pages": [
              "features/speech",
              "features/transcription",
              "features/translation",
              "features/tone"
            ]
          },
          {
            "group": "Concepts",
            "icon": "thought-bubble",
            "pages": [
              "concepts/voices",
              "concepts/languages",
              "concepts/pricing"
            ]
          }
        ]
      },
      {
        "dropdown": "API Reference",
        "icon": "display-code",
        "description": "spitch api reference",
        "groups": [
          {
            "group": "Endpoints",
            "openapi": {
              "source": "/api-reference/openapi.json"
            }
          },
          {
            "group": "Authentication",
            "pages": [
              "keys"
            ]
          }
        ]
      }
    ]
  },

  "footer": {

  },

  "thumbnails": {
    "background": "/images/thumbnail/background.svg"
  },
  "contextual": {
    "options": [
      "copy",
      "chatgpt",
      "claude",
      "cursor"
    ]
  },

  "logo": {
    "light": "/logo/logo-dark.png",
    "dark": "/logo/logo-light.png",
    "href": "https://docs.spitch.app"
  },
  "styling": {
    "eyebrows": "breadcrumbs"
  }

}